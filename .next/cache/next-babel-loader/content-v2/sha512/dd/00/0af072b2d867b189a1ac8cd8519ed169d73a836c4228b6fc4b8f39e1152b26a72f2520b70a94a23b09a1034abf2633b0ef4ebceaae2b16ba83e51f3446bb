{"ast":null,"code":"export function createSelectOption(option) {\n  return {\n    value: option,\n    label: option\n  };\n}\nexport function createSelectOptions(options) {\n  return options.map(function (option) {\n    return createSelectOption(option);\n  });\n}\nexport var getLabelStatus = function getLabelStatus(status) {\n  switch (status) {\n    case 'PUBLISHED':\n      return 'Опубликовано';\n\n    case 'MODERATION':\n      return 'На модерации';\n\n    default:\n      return status;\n  }\n};\nexport var getLabelRole = function getLabelRole(role) {\n  switch (role) {\n    case 'ADMIN':\n      return 'Админ';\n\n    case 'USER':\n      return 'Пользователь';\n\n    case 'MODERATOR':\n      return 'Модератор';\n\n    case 'OFICIAL':\n      return 'Оф. лицо';\n\n    case 'ENTITY':\n      return 'Юр. лицо';\n\n    case 'INDIVIDUAL':\n      return 'Физ. лицо';\n\n    default:\n      return role;\n  }\n};\nexport var getLabelCategory = function getLabelCategory(category) {\n  switch (category) {\n    case 'TICKET':\n      return 'Тема обращения';\n\n    case 'DIVISION':\n      return 'Раздел проектов/статей';\n\n    default:\n      return category;\n  }\n};\nexport var getLabelPermission = function getLabelPermission(permission) {\n  switch (permission) {\n    case 'ACCESS_CLIENT':\n      return 'Доступ к сайту';\n\n    case 'ACCESS_DASHBOARD':\n      return 'Доступ к административной панели';\n\n    case 'VIEW_USER':\n      return 'Просмотр пользователя';\n\n    case 'VIEW_CATEGORY':\n      return 'Просмотр категории';\n\n    case 'VIEW_ARTICLE':\n      return 'Просмотр статьи';\n\n    case 'VIEW_PROJECT':\n      return 'Просмотр проекта';\n\n    case 'VIEW_TICKET':\n      return 'Просмотр обращения';\n\n    case 'VIEW_ROLE':\n      return 'Просмотр роли';\n\n    case 'ADD_USER':\n      return 'Создание пользователя';\n\n    case 'ADD_CATEGORY':\n      return 'Создание категории';\n\n    case 'ADD_ARTICLE':\n      return 'Создание статьи';\n\n    case 'ADD_PROJECT':\n      return 'Создание проекта';\n\n    case 'EDIT_USER':\n      return 'Редактирвоание пользователя';\n\n    case 'EDIT_CATEGORY':\n      return 'Редактирование категории';\n\n    case 'EDIT_ARTICLE':\n      return 'Редактирвоание статьи';\n\n    case 'EDIT_PROJECT':\n      return 'Редактирвоание проекта';\n\n    case 'DELETE_USER':\n      return 'Удаление пользователя';\n\n    case 'DELETE_CATEGORY':\n      return 'Удаление категории';\n\n    case 'DELETE_ARTICLE':\n      return 'Удаление статьи';\n\n    case 'DELETE_PROJECT':\n      return 'Удаление проекта';\n\n    case 'COMMENT_ARTICLE':\n      return 'Комментирование статьи';\n\n    case 'COMMENT_PROJECT':\n      return 'Комментирование проекта';\n\n    case 'CHATTING':\n      return 'Общение';\n\n    default:\n      return 'Не определено';\n  }\n};\nexport var getFileSize = function getFileSize(size) {\n  if (!size) return 'Неизвестно';\n  var limit = 1024;\n  var bytes = Number(size);\n  var kilobytes = bytes / limit;\n  var megabytes = kilobytes / limit;\n  var gigabytes = megabytes / limit;\n  if (Math.trunc(gigabytes) > 0) return \"\".concat(Math.round(gigabytes), \" \\u0413\\u0411\");\n  if (Math.trunc(megabytes) > 0) return \"\".concat(Math.round(megabytes), \" \\u041C\\u0411\");\n  if (Math.trunc(kilobytes) > 0) return \"\".concat(Math.round(kilobytes), \" \\u041A\\u0411\");\n  return \"\".concat(bytes, \" \\u0431\\u0430\\u0439\\u0442\");\n};","map":{"version":3,"sources":["C:/Users/dan82/Documents/workspace/FREELANCE/atomic/atomic-frontend/atomic-ui/utils/functions.js"],"names":["createSelectOption","option","value","label","createSelectOptions","options","map","getLabelStatus","status","getLabelRole","role","getLabelCategory","category","getLabelPermission","permission","getFileSize","size","limit","bytes","Number","kilobytes","megabytes","gigabytes","Math","trunc","round"],"mappings":"AAAA,OAAO,SAASA,kBAAT,CAA4BC,MAA5B,EAAoC;AACzC,SAAO;AACLC,IAAAA,KAAK,EAAED,MADF;AAELE,IAAAA,KAAK,EAAEF;AAFF,GAAP