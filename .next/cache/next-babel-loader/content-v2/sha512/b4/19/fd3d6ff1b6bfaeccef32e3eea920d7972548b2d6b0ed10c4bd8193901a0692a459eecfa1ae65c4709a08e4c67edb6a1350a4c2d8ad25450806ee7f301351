S,\n      variables: {\n        account: ['ENTITY']\n      }\n    })\n\n    if (response && response.data) {\n      companies = response.data.getUsers\n    }\n  } catch (err) {\n    console.log(err)\n  }\n\n  return {\n    props: {\n      store: {\n        users,\n        companies\n      }\n    }\n  }\n}\n\nexport default Creators\n"]},"metadata":{},"sourceType":"module"}