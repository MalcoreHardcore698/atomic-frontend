{"ast":null,"code":"/* eslint-disable */\nimport C from '../types/root';\nconst initialState = {\n  mutation: null,\n  variables: null,\n  callback: () => {},\n  members: [],\n  screenshots: [],\n  messages: [],\n  files: []\n};\nexport const rootReducer = (state = initialState, action) => {\n  switch (action.type) {\n    case C.SET_MUTATE:\n      return { ...state,\n        ...action.payload\n      };\n\n    case C.SET_MEMBERS:\n      return { ...state,\n        members: action.payload.members || []\n      };\n\n    case C.SET_SCREENSHOTS:\n      return { ...state,\n        screenshots: action.payload.screenshots || []\n      };\n\n    case C.SET_FILES:\n      return { ...state,\n        files: action.payload.files || []\n      };\n\n    case C.ADD_MEMBER:\n      return { ...state,\n        members: [...state.members, action.payload.member]\n      };\n\n    case C.SET_FOLDER:\n      return { ...state,\n        folder: action.payload.folder || []\n      };\n\n    case C.ADD_SCREENSHOT:\n      return { ...state,\n        screenshots: [...state.screenshots, action.payload.screenshot]\n      };\n\n    case C.SET_CHAT:\n      return { ...state,\n        chat: action.payload.chat\n      };\n\n    case C.SET_MESSAGES:\n      return { ...state,\n        messages: action.payload.messages\n      };\n\n    case C.ADD_FILE:\n      return { ...state,\n        files: [...state.files, action.payload.file]\n      };\n\n    case C.REMOVE_MEMBER:\n      return { ...state,\n        members: state.members.filter(member => member.value.email !== action.payload.email)\n      };\n\n    case C.REMOVE_SCREENSHOT:\n      return { ...state,\n        screenshots: state.screenshots.filter(screenshot => screenshot.id !== action.payload.id)\n      };\n\n    case C.REMOVE_FILE:\n      return { ...state,\n        files: state.files.filter(file => file.id !== action.payload.id)\n      };\n\n    case C.CLEAR_MEMBERS:\n      return { ...state,\n        members: []\n      };\n\n    case C.CLEAR_SCREENSHOTS:\n      return { ...state,\n        screenshots: []\n      };\n\n    case C.CLEAR_FILES:\n      return { ...state,\n        files: []\n      };\n\n    default:\n      return state;\n  }\n};\nexport default rootReducer;","map":{"version":3,"sources":["C:/Users/dan82/Documents/workspace/FREELANCE/atomic/atomic-frontend/store/reducers/root.js"],"names":["C","initialState","mutation","variables","callback","members","screenshots","messages","files","rootReducer","state","action","type","SET_MUTATE","payload","SET_MEMBERS","SET_SCREENSHOTS","SET_FILES","ADD_MEMBER","member","SET_FOLDER","folder","ADD_SCREENSHOT","screenshot","SET_CHAT","chat","SET_MESSAGES","ADD_FILE","file","REMOVE_MEMBER","filter","value","email","REMOVE_SCREENSHOT","id","REMOVE_FILE","CLEAR_MEMBERS","CLEAR_SCREENSHOTS","CLEAR_FILES"],"mappings":"AAAA;AACA,OAAOA,CAAP,MAAc,eAAd;AAEA,MAAMC,YAAY,GAAG;AACnBC,EAAAA,QAAQ,EAAE,IADS;AAEnBC,EAAAA,SAAS,EAAE,IAFQ;AAGnBC,EAAAA,QAAQ,EAAE,MAAM,CAAE,CAHC;AAInBC,EAAAA,OAAO,EAAE,EAJU;AAKnBC,EAAAA,WAAW,EAAE,EALM;AAMnBC,EAAAA,QAAQ,EAAE,EANS;AAOnBC,EAAAA,KAAK,EAAE;AAPY,CAArB;AAUA,OAAO,MAAMC,WAAW,GAAG,CAACC,KAAK,GAAGT,YAAT,EAAuBU,MAAvB,KAAkC;AAC3D,UAAQA,MAAM,CAACC,IAAf;AACE,SAAKZ,CAAC,CAACa,UAAP;AACE,aAAO,EACL,GAAGH,KADE;AAEL,WAAGC,MAAM,CAACG;AAFL,OAAP;;AAIF,SAAKd,CAAC,CAACe,WAAP;AACE,aAAO,EACL,GAAGL,KADE;AAELL,QAAAA,OAAO,EAAEM,MAAM,CAACG,OAAP,CAAeT,OAAf,IAA0B;AAF9B,OAAP;;AAIF,SAAKL,CAAC,CAACgB,eAAP;AACE,aAAO,EACL,GAAGN,KADE;AAELJ,QAAAA,WAAW,EAAEK,MAAM,CAACG,OAAP,CAAeR,WAAf,IAA8B;AAFtC,OAAP;;AAIF,SAAKN,CAAC,CAACiB,SAAP;AACE,aAAO,EACL,GAAGP,KADE;AAELF,QAAAA,KAAK,EAAEG,MAAM,CAACG,OAAP,CAAeN,KAAf,IAAwB;AAF1B,OAAP;;AAIF,SAAKR,CAAC,CAACkB,UAAP;AACE,aAAO,EACL,GAAGR,KADE;AAELL,QAAAA,OAAO,EAAE,CACP,GAAGK,KAAK,CAACL,OADF,EAEPM,MAAM,CAACG,OAAP,CAAeK,MAFR;AAFJ,OAAP;;AAOF,SAAKnB,CAAC,CAACoB,UAAP;AACE,aAAO,EACL,GAAGV,KADE;AAELW,QAAAA,MAAM,EAAEV,MAAM,CAACG,OAAP,CAAeO,MAAf,IAAyB;AAF5B,OAAP;;AAIF,SAAKrB,CAAC,CAACsB,cAAP;AACE,aAAO,EACL,GAAGZ,KADE;AAELJ,QAAAA,WAAW,EAAE,CACX,GAAGI,KAAK