       })\n      };\n\n    case C.REMOVE_SCREENSHOT:\n      return { ...state,\n        screenshots: state.screenshots.filter(screenshot => screenshot.id !== action.payload.id)\n      };\n\n    case C.REMOVE_FILE:\n      return { ...state,\n        files: state.files.filter(file => file.id !== action.payload.id)\n      };\n\n    case C.CLEAR_MEMBERS:\n      return { ...state,\n        members: []\n      };\n\n    case C.CLEAR_SCREENSHOTS:\n      return { ...state,\n        screenshots: []\n      };\n\n    case C.CLEAR_FILES:\n      return { ...state,\n        files: []\n      };\n\n    default:\n      return state;\n  }\n};\nexport default rootReducer;","map":{"version":3,"sources":["C:/Users/dan82/Documents/workspace/FREELANCE/atomic/atomic-frontend/store/reducers/root.js"],"names":["C","initialState","mutation","settings","general","logotype","meta","title","description","variables","scrollTop","callback","members","screenshots","messages","files","rootReducer","state","action","type","SET_MUTATE","payload","SET_SETTINGS","SET_MEMBERS","SET_SCREENSHOTS","SET_FILES","SET_FOLDER","folder","SET_CHAT","chat","SET_MESSAGES","ADD_MEMBER","member","ADD_SCREENSHOT","screenshot","ADD_FILE","file","REMOVE_MEMBER","filter","email","value","REMOVE_SCREENSHOT","id","REMOVE_FILE","CLEAR_MEMBERS","CLEAR_SCREENSHOTS","CLEAR_FILES"],"mappings":"AAAA;AACA,OAAOA,CAAP,MAAc,eAAd;AAEA,MAAMC,YAAY,GAAG;AACnBC,EAAAA,QAAQ,EAAE,IADS;AAEnBC,EAAAA,QAAQ,EAAE;AACRC,IAAAA,OAAO,EAAE;AACPC,MAAAA,QAAQ,EAAE,EADH,CACM;;AADN,KADD;AAIRC,IAAAA,IAAI,EAAE;AACJC,MAAAA,KAAK,EAAE,EADH;AAEJC,MAAAA,WAAW,EAAE;AAFT;AAJE,GAFS;AAWnBC,EAAAA,SAAS,EAAE,IAXQ;AAYnBC,EAAAA,SAAS,EAAE,CAZQ;AAanBC,EAAAA,QAAQ,EAAE,MAAM,CAAE,CAbC;AAcnBC,EAAAA,OAAO,EAAE,EAdU;AAenBC,EAAAA,WAAW,EAAE,EAfM;AAgBnBC,EAAAA,QAAQ,EAAE,EAhBS;AAiBnBC,EAAAA,KAAK,EAAE;AAjBY,CAArB;AAoBA,OAAO,MAAMC,WAAW,GAAG,CAACC,KAAK,GAAGhB,YAAT,EAAuBiB,MAAvB,KAAkC;AAC3D,UAAQA,MAAM,CAACC,IAAf;AACE,SAAKnB,CAAC,CAACoB,UAAP;AACE,aAAO,EACL,GAAGH,KADE;AAEL,WAAGC,MAAM,CAACG;AAF