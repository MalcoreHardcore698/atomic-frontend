    categories = response.data.getCategories;\n    }\n  } catch (err) {\n    console.log(err);\n  }\n\n  return {\n    props: {\n      store: {\n        articles,\n        categories\n      }\n    }\n  };\n}\nexport default Articles;","map":{"version":3,"sources":["C:/Users/dan82/Documents/workspace/FREELANCE/atomic/atomic-frontend/pages/articles.js"],"names":["React","initializeApollo","useHelper","ContentLayout","ArticleCard","GridAside","Container","onArticleLink","queries","getLabelCategory","TITLE","START_OFFSET","Articles","store","recall","type","placeholder","options","categories","map","category","value","id","label","name","GET_ARTICLES","documents","articles","article","getServerSideProps","client","response","query","GET_META_ARTICLES","variables","offset","limit","status","data","getArticles","getCategories","err","console","log","props"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,SAASC,gBAAT,QAAiC,WAAjC;AACA,SAASC,SAAT,QAA0B,oBAA1B;AACA,OAAOC,aAAP,MAA0B,oBAA1B;AACA,OAAOC,WAAP,MAAwB,2BAAxB;AACA,SAASC,SAAS,IAAIC,SAAtB,QAAuC,sBAAvC;AACA,SAASC,aAAT,QAA8B,0BAA9B;AACA,OAAOC,OAAP,MAAoB,oBAApB;AACA,SAASC,gBAAT,QAAiC,8BAAjC;AAEA,MAAMC,KAAK,GAAG,QAAd;AACA,MAAMC,YAAY,GAAG,CAArB;;AAEA,MAAMC,QAAQ,GAAG,CAAC;AAAEC,EAAAA;AAAF,CAAD,KAAe;AAC9B,QAAMC,MAAM,GAAGZ,SAAS,EAAxB;AAEA,sBACE,oBAAC,aAAD;AACE,IAAA,KAAK,EAAEQ,KADT;AAEE,IAAA,OAAO,EAAE,CACP;AAAEK,MAAAA,IAAI,EAAE,YAAR;AAAsBC,MAAAA,WAAW,EAAE;AAAnC,KADO,EAEP;AACED,MAAAA,IAAI,EAAE,QADR;AAEEE,MAAAA,OAAO,EAAEJ,KAAF,aAAEA,KAAF,uBAAEA,KAAK,CAAEK,UAAP,CAAkBC,GAAlB,CAAuBC,QAAD,KAAe;AAC5CC,QAAAA,KAAK,EAAED,QAAQ,CAACE,EAD4B;AAE5CC,QAAAA,KAAK,EAAEd,gBAAgB,CAACW,QAAQ,CAACI,IAAV;AAFqB,OAAf,CAAtB;AAFX,KAFO,CAFX;AAYE,IAAA,OAAO,EAAE,CACP;AAAED,MAAAA,KAAK,EAAE,WAAT;AAAsBF,MAAAA,KAAK,EAAE;AAA7B,KADO,EAEP;AAAEE,MAAAA,KAAK,EAAE,iBAAT;AAA4BF,MAAAA,KAAK,EAAE;AAAnC,KAFO,CAZX;AAgBE,IAAA,KAAK,EAAEb,OAAO,CAACiB,YAhBjB;AAiBE,IAAA,KAAK,EAAE;AAAEC,MAAAA,SAAS,EAAEb,KAAF,aAAEA,KAAF,uBAAEA,KAAK,CAAEc;AAApB;AAjBT,KAkBG,CAAC;AAAED,IAAAA;AAAF,GAAD,kBAC