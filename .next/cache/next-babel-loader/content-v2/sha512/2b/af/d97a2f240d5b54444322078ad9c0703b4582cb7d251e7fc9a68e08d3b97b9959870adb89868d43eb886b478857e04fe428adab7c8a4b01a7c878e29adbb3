l={() => dispatch(setModal(null))}\n            onSubmit={async (_, action) => {\n              if (action) {\n                const response = await action({\n                  variables: { id }\n                })\n                dispatch(setUserFolders(response.data.deleteUserFolder))\n                dispatch(setModal(null))\n                if (callback) callback()\n              }\n            }}\n          />\n        )\n      }\n    ])\n  )\n}\n"]},"metadata":{},"sourceType":"module"}